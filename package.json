{
  "name": "tinker-p4t",
  "version": "0.0.556",
  "description": "secret...",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "bump": "bumpp patch --commit --tag --push --yes",
    "deploy-app": "cd dist; ln -s ../.vercel; vc --prod",
    "release": "pnpm bump && pnpm build && pnpm deploy-app",
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently \"pnpm:twx --watch\" \"pnpm:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "cc": "prettier --check './src/**/*.{css,pcss,js,jsx,ts,tsx}'",
    "format": "prettier --write './src/**/*.{css,pcss,js,jsx,ts,tsx}'",
    "spell": "cspell './**/*.{js,jsx,ts,tsx}'",
    "build": "pnpm twx; pnpm format; tsc && vite build",
    "preview": "vite preview",
    "open": "open https://tinker-p4t.vercel.app"
  },
  "dependencies": {
    "preact": "10.28.3"
  },
  "devDependencies": {
    "@preact/preset-vite": "2.10.3",
    "@preact/signals": "2.6.2",
    "autoprefixer": "10.4.24",
    "bumpp": "10.4.0",
    "concurrently": "9.2.1",
    "cspell": "9.6.2",
    "magic-string": "0.30.21",
    "only-allow": "1.2.2",
    "pettier": "1.1.0",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "proto-tailwindcss-clrs": "0.0.464",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "7.3.1"
  }
}
